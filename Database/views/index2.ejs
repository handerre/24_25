<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Brukere og Kjøretøy</title>
</head>
<body>
    <h1>Liste over brukere</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <strong>Navn:</strong> <%= user.fornavn %> <%= user.etternavn %><br>
                <strong>Mobil:</strong> <%= user.mobil %><br>
                <strong>Adresse:</strong> <%= user.gateadresse %> <%= user.gatenummer %>, <%= user.poststed %> (<%= user.postnummer %>)<br>
                <strong>E-post:</strong> <%= user.email %><br>
                <strong>Registrerte biler:</strong>
                <ul>
                    <% user.biler.forEach(bil => { %>
                        <li>
                            <strong>Registreringsnummer:</strong> <%= bil.registreringsnummer %><br>
                            <strong>Merke:</strong> <%= bil.merke %><br>
                            <strong>Modell:</strong> <%= bil.modell %><br>
                            <strong>Kilometer:</strong> <%= bil.km %> km<br>
                            <strong>Drivstofftype:</strong> <%= bil.drivstofftype %><br>
                            <strong>Farge:</strong> <%= bil.farge %><br>
                            <strong>Registreringsdato:</strong> <%= bil.registreringsdato %><br>
                        </li>
                    <% }) %>
                </ul>
            </li>
        <% }) %>
    </ul>
</body>
</html>
